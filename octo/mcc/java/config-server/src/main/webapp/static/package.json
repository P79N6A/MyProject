    "grunt": "^0.4.5",
    "mtfe_build_site": "^0.3.5",
    "mtfe_cos-core": "0.0.58",
    "mtfe_cos-ui": "^1.2.13",
    "mtfe_cos_config": "^0.1.3",
    "mtfe_cos_mt-fe": "^1.6.3",
    "mtfe_cos_track": "0.0.15",
    "mtfe_fe.core": "^0.2.109",
