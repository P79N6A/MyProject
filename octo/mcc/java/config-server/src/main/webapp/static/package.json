{
  "name": "mtfe_cos_mtconfig",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "panel_synclog_template": "cat panel/synclog.micro | node_modules/.bin/template_compile --namespace mt.config.panel.synclog --name template --module config-panel/synclog-tpl > panel/synclog-tpl.js",
    "build": "grunt build_site:mtconfig",
    "deploy": "npm -r http://r.npm.sankuai.com i mtfe_cos_deploy@'>=0.3.0' && deploy",
    "deploy_mtconfig_dev": "NODE_ENV=development cos_deploy"
  },
  "config": {
    "registry": "http://r.npm.sankuai.com"
  },
  "deployStatic": {
    "basePathAccordingToNODE_ENV": true,
    "syncIncludes": [
      "/node_modules/mtfe_cos-core",
      "/node_modules/mtfe_fe.core",
      "/node_modules/mtfe_cos-ui",
      "/node_modules/mtfe_cos_track",
      "/node_modules/mtfe_yui-config"
    ]
  },
