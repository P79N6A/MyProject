How to make rpm

# install lib deps
yum install libzookeeper-devel rpm-build

# checkout code
git clone ssh://git@git.sankuai.com/inf/octo.git
cd octo
tar czf sg_agent.tar.gz sg_agent


mv sg_agent.tar.gz ~/rpmbuild/SOURCES/

# build source
rpmbuild -ba sg_agent/contrib/sg_agent.spec

# install rpm
rpm -ivh ~/rpmbuild/RPMS/x86_64/sg_agent-xxxx.rpm
