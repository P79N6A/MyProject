<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="clientProxy" class="com.meituan.service.mobile.mtthrift.proxy.ThriftClientProxy" destroy-method="destroy">
        <property name="appKey" value="com.sankuai.inf.mtthrift.testClient"/>
        <property name="remoteAppkey" value="com.sankuai.inf.mtthrift.testServer"/>
        <property name="serviceInterface" value="com.sankuai.mtthrift.testSuite.idlTest.Twitter"/>
        <property name="remoteServerPort" value="8004"/>
        <property name="timeout" value="5000"/>
        <property name="gzip" value="false"/>
        <property name="chenkSum" value="false"/>
    </bean>
</beans>