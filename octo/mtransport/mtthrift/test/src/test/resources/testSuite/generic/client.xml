<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="clientProxy" class="com.meituan.service.mobile.mtthrift.proxy.ThriftClientProxy" destroy-method="destroy">
        <property name="appKey" value="com.sankuai.inf.mtthrift.testClient"/>
        <property name="remoteAppkey" value="com.sankuai.inf.mtthrift.testServer"/>
        <property name="genericServiceName" value="com.sankuai.mtthrift.testSuite.generic.Generic"/>
        <property name="timeout" value="5000"/>
        <property name="localServerPort" value="9009"/>
        <property name="remoteUniProto" value="true"/>
        <property name="generic" value="json"/>

    </bean>
</beans>